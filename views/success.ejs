<%- include header %>
<p>Your Photo has been successfully cropped!</p>
<p>Click <a href="/">here</a> to Upload another photo.</p>
<ul class="thumbnails">
	<% results.forEach(function(result) { %>
	<li class="span3">
		<div class="thumbnail">
			<img src="<%- result.url %>" alt="<%- result.name %>" />
			<h4><%- result.name %></h4>
			<p>Saved as: <%- result.path %></p>
		</div>
	</li>
	<% }); %>
</ul>
<%- include footer %>